package lentolippuvaraus;

import java.util.ArrayList;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

			ArrayList<Paikka> paikat = new ArrayList<>();
			for(int i = 0; i<10;i++) {
				for (int j=0; j<4;j++){
				Paikka p=new Paikka(null,true,i,j);
				paikat.add(p);
				}
			}
			
			
		Lentokone lk = new Lentokone(paikat,Maat.RUOTSI);
		lk.toString();
	


		Ohjelma m = new Ohjelma();
		m.kaynnista();

 }
}
